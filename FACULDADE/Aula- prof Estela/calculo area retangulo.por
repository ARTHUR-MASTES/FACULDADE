programa
{
	
	funcao inicio()
	{
	real base, altura, area
	escreva("Digite a base do retangulo; ")
	leia(base)
	escreva("Digite a altura do retangulo: ")
	leia(altura)
	area = base * altura
	escreva("A area do retangulo e; ", area)
	
	}
}
